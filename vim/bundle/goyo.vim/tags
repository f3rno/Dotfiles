!_TAG_FILE_FORMAT	2	/extended format; --format=1 will not append ;" to lines/
!_TAG_FILE_SORTED	1	/0=unsorted, 1=sorted, 2=foldcase/
!_TAG_PROGRAM_AUTHOR	Darren Hiebert	/dhiebert@users.sourceforge.net/
!_TAG_PROGRAM_NAME	Exuberant Ctags	//
!_TAG_PROGRAM_URL	http://ctags.sourceforge.net	/official site/
!_TAG_PROGRAM_VERSION	5.8	//
<plug>(goyo-resize)	autoload/goyo.vim	/^nnoremap <silent> <plug>(goyo-resize) :<c-u>call <sid>resize_pads()<cr>$/;"	m
Goyo	plugin/goyo.vim	/^command! -nargs=? -bar -bang Goyo call goyo#execute(<bang>0, <q-args>)$/;"	c
blank	autoload/goyo.vim	/^function! s:blank(repel)$/;"	f
const	autoload/goyo.vim	/^function! s:const(val, min, max)$/;"	f
get_color	autoload/goyo.vim	/^function! s:get_color(group, attr)$/;"	f
goyo#execute	autoload/goyo.vim	/^function! goyo#execute(bang, dim)$/;"	f
goyo_off	autoload/goyo.vim	/^function! s:goyo_off()$/;"	f
goyo_on	autoload/goyo.vim	/^function! s:goyo_on(dim)$/;"	f
hide_linenr	autoload/goyo.vim	/^function! s:hide_linenr()$/;"	f
hide_statusline	autoload/goyo.vim	/^function! s:hide_statusline()$/;"	f
init_pad	autoload/goyo.vim	/^function! s:init_pad(command)$/;"	f
maps_nop	autoload/goyo.vim	/^function! s:maps_nop()$/;"	f
maps_resize	autoload/goyo.vim	/^function! s:maps_resize()$/;"	f
parse_arg	autoload/goyo.vim	/^function! s:parse_arg(arg)$/;"	f
relsz	autoload/goyo.vim	/^function! s:relsz(expr, limit)$/;"	f
resize_pads	autoload/goyo.vim	/^function! s:resize_pads()$/;"	f
s:cpo_save	autoload/goyo.vim	/^let s:cpo_save = &cpo$/;"	v
set_color	autoload/goyo.vim	/^function! s:set_color(group, attr, color)$/;"	f
setup_pad	autoload/goyo.vim	/^function! s:setup_pad(bufnr, vert, size, repel)$/;"	f
tranquilize	autoload/goyo.vim	/^function! s:tranquilize()$/;"	f
