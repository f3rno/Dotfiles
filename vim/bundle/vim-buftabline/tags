!_TAG_FILE_FORMAT	2	/extended format; --format=1 will not append ;" to lines/
!_TAG_FILE_SORTED	1	/0=unsorted, 1=sorted, 2=foldcase/
!_TAG_PROGRAM_AUTHOR	Darren Hiebert	/dhiebert@users.sourceforge.net/
!_TAG_PROGRAM_NAME	Exuberant Ctags	//
!_TAG_PROGRAM_URL	http://ctags.sourceforge.net	/official site/
!_TAG_PROGRAM_VERSION	5.8	//
BufTabLine	plugin/buftabline.vim	/^augroup BufTabLine$/;"	a
buftabline#render	plugin/buftabline.vim	/^function! buftabline#render()$/;"	f
buftabline#update	plugin/buftabline.vim	/^function! buftabline#update(zombie)$/;"	f
buftabline#user_buffers	plugin/buftabline.vim	/^function! buftabline#user_buffers() " help buffers are always unlisted, but quickfix buffers are not$/;"	f
g:buftabline_indicators	plugin/buftabline.vim	/^let g:buftabline_indicators = get(g:, 'buftabline_indicators', 0)$/;"	v
g:buftabline_numbers	plugin/buftabline.vim	/^let g:buftabline_numbers    = get(g:, 'buftabline_numbers',    0)$/;"	v
g:buftabline_plug_max	plugin/buftabline.vim	/^let g:buftabline_plug_max   = get(g:, 'buftabline_plug_max',  10)$/;"	v
g:buftabline_separators	plugin/buftabline.vim	/^let g:buftabline_separators = get(g:, 'buftabline_separators', 0)$/;"	v
g:buftabline_show	plugin/buftabline.vim	/^let g:buftabline_show       = get(g:, 'buftabline_show',       2)$/;"	v
s:b	plugin/buftabline.vim	/^	let s:b = s:n == -1 ? -1 : s:n - 1$/;"	v
s:centerbuf	plugin/buftabline.vim	/^let s:centerbuf = winbufnr(0)$/;"	v
s:dirsep	plugin/buftabline.vim	/^let s:dirsep = fnamemodify(getcwd(),':p')[-1:]$/;"	v
transpile	plugin/buftabline.vim	/^	function s:transpile()$/;"	f
